{
  "version": 2,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "unsafe-none"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "unsafe-none"
        },
        {
          "key": "Cross-Origin-Resource-Policy",
          "value": "cross-origin"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/telemetry/:path*",
      "destination": "https://fall-detection-system-6qw5.onrender.com/api/telemetry/:path*"
    },
    {
      "source": "/api/(auth/.*|users/.*|chatgpt/.*|devices/.*|events/.*)",
      "destination": "https://fall-detection-system-6qw5.onrender.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}